<p id="notice"><%= notice %></p>

<div class="users">
  <strong>Patients:</strong>
  <ul>
    <% @users.each do |user| %>
    <div class="strain col-md-3">
      Name: <%= link_to user.name, user_path(user) %><br />
      Address: <%= user.full_address %><br />
      Med Card: <%= user.med_card_number %><br />
      Plant Count: <%= user.plant_count %><br />
      User Name: <%= user.user_name %><br />
      Role: <%= user.role %><br />
      Dispensary: <%= user.user.dispensary.name %><br />
      <%= link_to 'Edit', edit_admin_user_path(user) %>
      <%= link_to 'Delete', admin_user_path(user), method: :delete, data: { confirm: 'Are you sure?' }%>
    </div>
    <% end %>
  </ul>
</div>

<%= link_to 'New User', new_admin_user_path %>
